{
  "version": 2,
  "builds": [
    { "src": "api/register.js", "use": "@vercel/node" },
    { "src": "api/send-email.js", "use": "@vercel/node" },
    { "src": "api/save-sms.js", "use": "@vercel/node" },
    { "src": "api/save-otp.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/register", "dest": "api/register.js" },
    { "src": "/api/send-email", "dest": "api/send-email.js" },
    { "src": "/api/save-sms", "dest": "api/save-sms.js" },
    { "src": "/api/save-otp", "dest": "api/save-otp.js" }
  ]
}
